{
  "name": "@wingsuit-designsystem/wingsuit",
  "version": "0.1.1-alpha.2",
  "description": "A starterkit to build a design system with Pattern Lab Tailwind and Drupal",
  "main": "",
  "license": "GPL-2.0",
  "author": {
    "name": "KEY-TEC",
    "url": "https://github.com/wingsuit-designsystem/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wingsuit-designsystem/wingsuit.git"
  },
  "keywords": [
    "storybook",
    "prototyping"
  ],
  "scripts": {
    "dev:storybook": "start-storybook --config-dir apps/storybook",
    "yo": "yo",
    "dev:drupal": "cross-env-shell NODE_ENV=development \"webpack --watch --config ./apps/drupal/webpack.config.js\"",
    "build:drupal": "cross-env-shell NODE_ENV=production \"webpack --config ./apps/drupal/webpack.config.js\"",
    "lint:js": "eslint --ext .js,.vue ./",
    "lint:css": "stylelint '**/*.css'",
    "lint": "npm run lint:js; npm run lint:css;",
    "fmt:js": "npm run lint:js -- --fix",
    "fmt:css": "npm run lint:css -- --fix && prettier --loglevel warn --write '**/*.{css}'",
    "fmt": "npm run fmt:js; npm run fmt:css",
    "test:backstop:ref": "node tools/tests/vrt/backstop-example-reference.js",
    "test:backstop:test": "node tools/tests/vrt/backstop-example-test.js",
    "test:pa11y": "pa11y-ci --config tools/tests/accessibility/pa11y.js",
    "test:unit:default": "NODE_ENV=test jest --projects source/default/",
    "test:unit": "npm run test:unit:default",
    "test": "npm run test:unit",
    "ci": "npm run lint && npm run test && npm run build:pl && npm run build:drupal",
    "new": "yo @wingsuit-designsystem/new-component && patternlab build --config ./apps/node-pl/patternlab-config.json"
  },
  "dependencies": {
    "alpinejs": "^1.10.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-backgrounds": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/html": "^5.3.17",
    "@wingsuit-designsystem/core": "0.1.1-alpha.2",
    "@wingsuit-designsystem/generator-new-component": "0.1.1-alpha.2",
    "@wingsuit-designsystem/storybook": "0.1.1-alpha.2",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "drupal-attribute": "^1.0.2",
    "enquire.js": "^2.1.6",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "holderjs": "^2.9.6",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-vue-preprocessor": "^1.7.1",
    "jsondiffpatch": "^0.3.11",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-pixels-to-rem": "^0.8.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-replace": "^1.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prismjs": "^1.17.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3",
    "resolve-url-loader": "^3.1.1",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "svg-spritemap-webpack-plugin": "^3.5.3",
    "svg4everybody": "^2.1.9",
    "tailwindcss": "^1.2.0",
    "terser-webpack-plugin": "^2.3.4",
    "twig-drupal-filters": "^2.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "vue-testing-library": "^0.17.0",
    "vuex": "^3.1.2",
    "webpack": "^4.41.3",
    "yo": "^3.1.1"
  }
}
