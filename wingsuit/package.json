{
  "name": "@wingsuit-designsystem/wingsuit",
  "version": "0.0.1",
  "description": "A starterkit to build a design system with Pattern Lab Tailwind and Drupal",
  "main": "",
  "license": "GPL-2.0",
  "author": {
    "name": "KEY-TEC",
    "url": "https://github.com/wingsuit-designsystem/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wingsuit-designsystem/wingsuit.git"
  },
  "keywords": [
    "pattern-lab",
    "prototyping"
  ],
  "scripts": {
    "patternlab": "patternlab --config ./apps/node-pl/patternlab-config.json",
    "start": "npm run dev:pl-node",
    "yo": "yo",
    "pl-node": "node ./apps/node-pl/pl-app.js",
    "setup": "patternlab install --config ./apps/node-pl/patternlab-config.json && patternlab build --config ./apps/node-pl/patternlab-config.json",
    "export:pl": "patternlab export --config ./apps/node-pl/patternlab-config.json",
    "dev:pl-node": "cross-env-shell NODE_ENV=development \"npm run pl-node & webpack-dev-server --config ./apps/node-pl/webpack.config.js --hot\"",
    "build:pl": "cross-env-shell NODE_ENV=production \"npm run pl-node && webpack --config ./apps/node-pl/webpack.config.js\"",
    "dev:build:pl": "cross-env-shell NODE_ENV=development \"npm run pl-node && webpack --config ./apps/node-pl/webpack.config.js\"",
    "dev:drupal": "cross-env-shell NODE_ENV=development \"webpack --watch --config ./apps/drupal/webpack.config.js\"",
    "build:drupal": "cross-env-shell NODE_ENV=production \"webpack --config ./apps/drupal/webpack.config.js\"",
    "lint:js": "eslint --ext .js,.vue ./",
    "lint:scss": "stylelint '**/*.{css,scss}'",
    "lint": "npm run lint:js; npm run lint:scss;",
    "fmt:js": "npm run lint:js -- --fix",
    "fmt:scss": "npm run lint:scss -- --fix && prettier --loglevel warn --write '**/*.{css,scss}'",
    "fmt": "npm run fmt:js; npm run fmt:scss",
    "test:backstop:ref": "node tools/tests/vrt/backstop-example-reference.js",
    "test:backstop:test": "node tools/tests/vrt/backstop-example-test.js",
    "test:pa11y": "pa11y-ci --config tools/tests/accessibility/pa11y.js",
    "test:unit:default": "NODE_ENV=test jest --projects source/default/",
    "test:unit": "npm run test:unit:default",
    "test": "npm run test:unit",
    "ci": "npm run lint && npm run test && npm run build:pl && npm run build:drupal",
    "new": "yo @wingsuit-designsystem/new-component && patternlab build --config ./apps/node-pl/patternlab-config.json"
  },
  "dependencies": {
    "@pattern-lab/cli": "^5.4.0",
    "@pattern-lab/core": "^5.4.0",
    "@pattern-lab/engine-twig-php": "^5.4.0",
    "@pattern-lab/uikit-workshop": "^5.5.0",
    "@wingsuit-designsystem/tools": "^0.0.4",
    "@wingsuit-designsystem/generator-new-component": "^0.0.2",
    "core-js": "^3.4.1",
    "enquire.js": "^2.1.6",
    "generate-json-webpack-plugin": "^0.3.1",
    "holderjs": "^2.9.6",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.0",
    "postcss-nested": "^4.2.1",
    "postcss-pxtorem": "^4.0.1",
    "prismjs": "^1.17.1",
    "regenerator-runtime": "^0.13.3",
    "svg4everybody": "^2.1.9",
    "tailwindcss": "^1.2.0-canary.5",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-vue-preprocessor": "^1.7.0",
    "jsondiffpatch": "^0.3.11",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-pixels-to-rem": "^0.8.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-replace": "^1.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "resolve-url-loader": "^3.1.1",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "svg-spritemap-webpack-plugin": "^3.5.3",
    "terser-webpack-plugin": "^2.3.1",
    "url-loader": "^3.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "vue-testing-library": "^0.17.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "yo": "^3.1.1"
  }
}
